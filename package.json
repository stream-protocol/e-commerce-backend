{
  "name": "medusa-backend",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Sebastian Rindom <skrindom@gmail.com>",
  "license": "MIT",
  "keywords": [
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "clean": "./node_modules/.bin/rimraf dist",
    "build": "babel src -d dist --extensions \".ts,.js\"",
    "serve": "medusa start",
    "watch": "tsc --watch",
    "test": "jest",
    "seed": "medusa seed -f ./data/seed.json",
    "start": "medusa develop",
    "heroku-postbuild": "medusa migrations run",
    "start:custom": "cross-env npm run build && node --preserve-symlinks index.js",
    "dev": "npm run build && medusa develop",
    "prepare": "npm run build",
    "build:admin": "medusa-admin build"
  },
  "dependencies": {
    "@medusajs/admin": "^6.0.5",
    "@medusajs/cache-inmemory": "^1.8.8",
    "@medusajs/cache-redis": "^1.8.8",
    "@medusajs/event-bus-local": "^1.9.6",
    "@medusajs/event-bus-redis": "^1.8.8",
    "@medusajs/file-local": "^1.0.1",
    "@medusajs/medusa": "^1.13.1",
    "@medusajs/types": "^1.10.1",
    "babel-preset-medusa-package": "^1.1.19",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.17.2",
    "medusa-file-minio": "^1.2.2",
    "medusa-fulfillment-manual": "^1.1.37",
    "medusa-interfaces": "^1.3.7",
    "medusa-payment-manual": "^1.0.24",
    "medusa-payment-stripe": "^6.0.2",
    "medusa-plugin-meilisearch": "^2.0.10",
    "medusa-plugin-segment": "^1.3.7",
    "medusa-plugin-sendgrid": "^1.3.11",
    "openai": "^3.3.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@medusajs/medusa-cli": "^1.3.18",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "mongoose": "^7.2.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^5.1.6"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
